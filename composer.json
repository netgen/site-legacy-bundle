{
    "name": "netgen/more-legacy-bundle",
    "description": "Netgen More Legacy Bundle is an eZ Platform bundle that implements common features that are used throughout Netgen's websites. This specific bundle acts as a glue between new and legacy kernels.",
    "authors": [
        {
            "name": "Netgen",
            "homepage": "http://www.netgenlabs.com"
        }
    ],
    "require": {
        "netgen/more-bundle": "~4.0.0",
        "ezsystems/legacy-bridge": "^2.0",
        "netgen/richtext-datatype-bundle": "^1.0",
        "ezsystems/eztags-ls": "^2.1",
        "netgen/ngsymfonytools": "^1.1",
        "netgen/ezplatformsearch": "^1.0",
        "netgen/enhancedselection2": "^2.0",
        "netgen/ngclasslist": "^1.0",
        "netgen/birthday": "^1.0",

        "netgen/bccie": "^1.0",
        "netgen/childrenindexer": "^1.0",
        "netgen/parentindexer": "^1.0",
        "netgen/hideuntildate": "^1.0",
        "netgen/ezclasslists": "^1.2",
        "netgen/ezchangeclass": "^1.0",
        "netgen/xrowmetadata": "^2.1"
    },
    "conflict": {
        "netgen/more-admin-ui-bundle": "*"
    },
    "autoload": {
        "psr-4": { "Netgen\\Bundle\\MoreLegacyBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
